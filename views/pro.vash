  <script>
    /* validation */
    $("#register-form").validate({
      rules: {
        name: {
          required: true,
          minlength: 3,
        },
      },
      messages: {
        name: "الرجاء كتابة الاسم",
      },
      submitHandler: submitForm,
    });
    /* validation */
    function submitForm() {
      var data = $("#register-form").serialize();

      $.ajax({
        type: "POST",
        url: "/sendOrder",
        data: data,
        beforeSend: function () {
          $("#error").fadeOut();
          $("#btn-submit").html(
            '<span class="glyphicon glyphicon-transfer"></span>   جاري الارسال ...'
          );
        },

        success: function (data) {
          console.log('data from ajax', data.data)
          if (data.data == 1) {
            console.log("data  === 1")

            $("#error").fadeIn(1000, function () {
              $("#error").html(
                '<div class="alert alert-danger"> <span class="glyphicon glyphicon-info-sign"></span>  تم الارسال سابقاً</div>'
              );

              $("#btn-submit").html(
                '<span class="glyphicon glyphicon-log-in"></span>  انشاء رسالة'
              );
            });
          } else if (data.data == "registered") {
            console.log("enter to  register")
            $("#btn-submit").html("تم تقديم الطلب");
            setTimeout(
              '$(".form-signin").fadeOut(200, function(){ $(".signin-form").load("index.php"); }); ',
              2000
            );
            html =
              '<div class="col-md-12 alert alert-block alert-danger">' +
              '<div  class="xpho">' +
              'حوالة مصرفية مباشرة<br/>قم بإجراء حوالة مباشرة لأحد حاسباتنا المصرفية، الرجاء استخدام رقم طلبك كمرجع لعملية الدفع، لن يتم شحن طلبك حتى يتم التأكد من عملية الدفع، ويجب التواصل واتس اب وارفاق صورة التحويل البنكي على واتس اب والبيانات الازمه، مع ذكر اللوكيشن والاسم الثلاثي ورقم الجوال.<br/>سيتم استخدام بياناتك الشخصية لمعالجة طلبك، ودعم تجربتك في هذا الموقع، ولأغراض أخرى تم توضيحها في سياسة الخصوصية لدينا.<br/><i class="ace-icon fa fa-check"></i>تم استقبال الطلب بنجاح<br/>يعتمد التوصيل عند تحويل قيمة الجهاز  ' +
              "</div>" +
              "</div>";
            $("#sms-alert-section").append($(html));
          } else {
            $("#error").fadeIn(1000, function () {
              $("#error").html(
                '<div class="alert alert-danger"><span class="glyphicon glyphicon-info-sign"></span>   ' +
                data +
                " !</div>"
              );

              $("#btn-submit").html(
                '<span class="glyphicon glyphicon-log-in"></span>   انشاء حساب'
              );
            });
          }
        },
      });
      return false;
    }

    /* Whatsapp Chat Widget by www.bloggermix.com */
    $(document).on("click", "#send-it", function () {
      var a = document.getElementById("chat-input");
      if ("" != a.value) {
        var b = $("#get-number").text(),
          c = document.getElementById("chat-input").value,
          d = "https://web.whatsapp.com/send",
          e = b,
          f = "&text=" + c;
        if (
          /Android|webOS|iPhone|iPad|iPod|BlackBerry|IEMobile|Opera Mini/i.test(
            navigator.userAgent
          )
        )
          var d = "whatsapp://send";
        var g = d + "?phone=+966560490595" + e + f;
        window.open(g, "_blank");
      }
    }),
      $(document).on("click", ".informasi", function () {
        (document.getElementById("get-number").innerHTML = $(this)
          .children(".my-number")
          .text()),
          $(".start-chat,.get-new").addClass("show").removeClass("hide"),
          $(".home-chat,.head-home").addClass("hide").removeClass("show"),
          (document.getElementById("get-nama").innerHTML = $(this)
            .children(".info-chat")
            .children(".chat-nama")
            .text()),
          (document.getElementById("get-label").innerHTML = $(this)
            .children(".info-chat")
            .children(".chat-label")
            .text());
      }),
      $(document).on("click", ".close-chat", function () {
        $("#whatsapp-chat").addClass("hide").removeClass("show");
      }),
      $(document).on("click", ".blantershow-chat", function () {
        $("#whatsapp-chat").addClass("show").removeClass("hide");
      });
  </script>

